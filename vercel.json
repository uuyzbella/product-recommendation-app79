{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@3"
    },
    "index.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "DIFY_REGION": "china",
    "LANGFUSE_ENABLED": "false"
  }
}