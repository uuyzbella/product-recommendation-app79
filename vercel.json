{
  "version": 2,
  "builds": [
    {
      "src": "dify-integration/vercel-adapter.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "/dify-integration/vercel-adapter.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/dify-integration/vercel-adapter.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "DIFY_REGION": "china",
    "LANGFUSE_ENABLED": "false"
  }
}