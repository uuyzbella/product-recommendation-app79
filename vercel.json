{
  "version": 2,
  "functions": {
    "dify-integration/vercel-adapter.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dify-integration/vercel-adapter.js"
    },
    {
      "src": "/health",
      "dest": "/dify-integration/vercel-adapter.js"
    },
    {
      "src": "/",
      "dest": "/dify-integration/vercel-adapter.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
